{
	"compilerOptions": {
		"target": "ES2021",
		"module": "ES2022",
		"lib": ["ES2021"],
		"types": ["@cloudflare/workers-types"],
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowJs": true,
		"noEmit": true,
		"isolatedModules": true,
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	},
	"include": ["index.ts"]
}
